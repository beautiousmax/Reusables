[report]
omit =
    */python?.?/*
    */site-packages/nose/*
    */test/*
    */pypy/*
    */nose/*
exclude_lines =
    pragma: no cover
    except Exception
    conn.close
    raise BrowserException
    object.__setattr__
    raise AttributeError
    object.__delattr__
    elif "darwin"
    return "mac"
    elif _sys.platform
    return "windows"
    except OSError as err
    raise err
    except NameError
    def _background_runner
    raise TypeError
    self.free_tasks
    self.busy_tasks
    self.current_tasks
    except KeyError
    if size < self.max_tasks
    elif size > self.max_tasks
    raise NotImplementedError
    return self.result_queue
    except (Exception, KeyboardInterrupt)
    if _os.path.isfile
    elif _os.path.isfile
    raise InvalidSchema
    pass
    _logger
    t += 1
    us = 0
